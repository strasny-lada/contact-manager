<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{{ 'app.title' | trans ~ (section_title is defined and section_title is not empty ? ' - ' ~ section_title : '') }}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body data-general-error="{{ {'name': 'app.generalError.name' | trans, 'message': 'app.generalError.message' | trans }|json_encode }}">
    <div class="container-fluid mt-3">
        <h1 class="mx-lg-5 main-headline"><a href="{{ path('contact_list') }}">{{ 'app.title' | trans }}</a></h1>
        <hr class="my-4" />

        {% block body %}{% endblock %}
    </div>

    {% include '_modal.html.twig' %}

    </body>
</html>
